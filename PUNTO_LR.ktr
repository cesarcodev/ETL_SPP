    <sql>SELECT lrd.num_listarec
	  ,lrd.num_linea_lr
	  ,lrd.posicion_visible
	  ,lrd.num_cia_serv
	  ,lrd.num_ofic_serv
	  ,lrd.num_ruta_oper
	  ,lrd.num_punto
	  ,lrd.observacion
	  ,lrd.num_cli
	  ,CAST(lrd.fecha_listarec as DATETIME) as fecha_listarec
	  ,TRIM(punto.nombre_punto) nombre_punto
	  , 'Sistema' as USUARIO_CREACION
, 'Sistema' as USUARIO_MODIFICACION
, CURRENT_TIMESTAMP as FEC_CREACION 
, CURRENT_TIMESTAMP as FEC_MODIFICACION 
, 4 as FK_ESTATUS
, 1 as ESTATUS_REGISTRO
  FROM re_lista_rec_d lrd 
  JOIN re_lista_rec_r lrr 
	ON lrd.num_cia_serv   = lrr.num_cia_serv
   AND lrd.num_ofic_serv  = lrr.num_ofic_serv
   AND lrd.num_ruta_oper  = lrr.num_ruta_oper
   AND lrd.fecha_listarec = lrr.fecha_listarec
   AND lrd.num_listarec   = lrr.num_listarec
  LEFT JOIN punto punto
    ON lrd.num_punto = punto.num_pto
where lrr.fecha_listarec = '2023-07-03'
--where lrr.fecha_listarec = CONVERT(DATE, CURRENT_TIMESTAMP) AND lrr.status_lr = 11
--where lrr.fecha_listarec = CONVERT(DATE, CURRENT_TIMESTAMP)

