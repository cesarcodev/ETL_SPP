    <connection>DEMO</connection>
    <sql>
SELECT
 CAST(lrd.fecha_listarec as DATETIME) as fecha_listarec
, lrd.num_listarec
,lrd.num_cia_serv
,lrd.num_ofic_serv
,lrd.num_ruta_oper
 ,lrr.status_lr
FROM re_lista_rec_d lrd
    ----------------------detalle de la lista
  JOIN re_lista_rec_r lrr
  ON lrd.num_cia_serv   = lrr.num_cia_serv
   AND lrd.num_ofic_serv  = lrr.num_ofic_serv
   AND lrd.num_ruta_oper  = lrr.num_ruta_oper
   AND lrd.fecha_listarec = lrr.fecha_listarec
   AND lrd.num_listarec   = lrr.num_listarec
--where lrr.fecha_listarec = '2023-06-27'
where  lrr.fecha_listarec = CONVERT(DATE, CURRENT_TIMESTAMP)
group by lrd.num_cia_serv, lrd.num_ofic_serv, lrd.num_ruta_oper, lrd.fecha_listarec, lrd.num_listarec,lrr.status_lr
order by lrd.num_cia_serv, lrd.num_ofic_serv, lrd.num_ruta_oper, lrd.fecha_listarec, lrd.num_listarec,lrr.status_lr</sql>
